@model IEnumerable<BabyCiao.Models.NannyRequirment>


<h1>保母審核</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                審核
            </th>
            <th>
                審核時間</br>
                狀態
            </th>
            <th>
                申請時間</br>
                申請帳號</br>
                申請地址
            </th>
            <th>
                良民證
            </th>
            <th>
                保母證
            </th>
            <th>
                身分證
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">審核</a><br />
                    <a asp-action="Delete" asp-route-id="@item.Id">退件</a>
                </td>
                <td>
                @Html.DisplayFor(modelItem => item.ValidPeriodsOfCertificates)<br />
                @Html.DisplayFor(modelItem => item.Statement)
                </td>
                 <td>
                @Html.DisplayFor(modelItem => item.RequirementDate)<br />
                @Html.DisplayFor(modelItem => item.NannyAccountUserAccountNavigation.Account)<br />
                @Html.DisplayFor(modelItem => item.AddressesOfAgencies)
                </td>
                 <td>
                @Html.DisplayFor(modelItem => item.PoliceCriminalRecordCertificate)
                 </td>
                  <td>
                @Html.DisplayFor(modelItem => item.ChildCareCertificate)
                 </td>
                  <td>
                @Html.DisplayFor(modelItem => item.NationalIdentificationCard)
                 </td>
       </tr>}
  </tbody>
</table>
