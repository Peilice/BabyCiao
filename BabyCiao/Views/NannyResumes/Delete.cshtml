@model BabyCiao.Models.NannyResume

<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <h1 class="h5 mt-4">@ViewData["Title"]</h1>
            <div class="card">
                <div class="card-body p-3">
                    <h4 class="card-title">確定要刪除這個項目嗎？</h4>
                    <hr />
                    <dl class="row">
                        <dt class="col-sm-2">帳號</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.NannyAccountUserAccountNavigation.Account)</dd>

                        <dt class="col-sm-2">城市</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.City)</dd>

                        <dt class="col-sm-2">區域</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.District)</dd>

                        <dt class="col-sm-2">簡介</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.Introduction)</dd>

                        <dt class="col-sm-2">托育類型</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.TypeOfDaycare)</dd>

                        <dt class="col-sm-2">服務項目</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.ServiceItems)</dd>

                        <dt class="col-sm-2">準公共托育</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.QuasiPublicChildcare)</dd>

                        <dt class="col-sm-2">可托育2歲以下</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.ChildcareAvailableUnder2)</dd>

                        <dt class="col-sm-2">可托育2歲以上</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.ChildcareAvailableOver2)</dd>

                        <dt class="col-sm-2">語言</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.Language)</dd>

                        <dt class="col-sm-2">服務中心</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.ServiceCenter)</dd>

                        <dt class="col-sm-2">專業肖像</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.ProfessionalPortrait)</dd>

                        <dt class="col-sm-2">內部照片1</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.InternalPhoto1)</dd>

                        <dt class="col-sm-2">內部照片2</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.InternalPhoto2)</dd>

                        <dt class="col-sm-2">內部照片3</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.InternalPhoto3)</dd>

                        <dt class="col-sm-2">內部照片4</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.InternalPhoto4)</dd>

                        <dt class="col-sm-2">內部照片5</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.InternalPhoto5)</dd>

                        <dt class="col-sm-2">顯示控制</dt>
                        <dd class="col-sm-10">@Html.DisplayFor(model => model.DisplayControl)</dd>

                        
                    </dl>

                    <form asp-action="Delete" method="post">
                        <input type="hidden" asp-for="Id" />
                        <input type="submit" value="刪除" class="btn btn-danger btn-sm" /> |
                        <a asp-action="Index" class="btn btn-secondary btn-sm">返回列表</a>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
